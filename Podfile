pod ‘Firebase/Storage’
pod 'FBSDKCoreKit'
pod 'FBSDKLoginKit'
pod 'FBSDKShareKit'
pod 'OneSignal', '>= 2.6.2', '< 3.0'
end
use_frameworks!
target 'OneSignalNotificationServiceExtension' do
    pod 'OneSignal', '>= 2.6.2', '< 3.0'
end
